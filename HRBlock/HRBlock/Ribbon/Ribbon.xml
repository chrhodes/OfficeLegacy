<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui" onLoad="OnLoad">
  <ribbon>
    <tabs>
		<tab id="HRBlockTab" label="HRBlock">
			<group id="HRBlockGroup" label="HRBlock" visible="true">
				<button id="Config"
						label="Config"
						screentip="Display Config Task Pane"
						onAction="TaskPane_Config_Click"
						imageMso="HappyFace"/>
				
				<button id="CreateSheets"
						label="CreateSheets"
						screentip="Display CreateSheets Task Pane"
						onAction="TaskPane_CreateSheets_Click"
						imageMso="HappyFace"/>	
				
				<button id="HRBlockButton"
						label="HR Block"
						screentip="Display HRB Task Pane"
						onAction="TaskPane_HRB_Click"
						imageMso="HappyFace"/>	
				
				<button id="Help"
						label="Help"
						screentip="Display Help Task Pane"
						onAction="TaskPane_Help_Click"
						imageMso="HappyFace"/>
			</group>
		</tab>
    </tabs>
  </ribbon>
</customUI>
